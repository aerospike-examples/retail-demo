p resistant outsole <br /> </span><span style=\"font-style: normal  font-weight;\">· </span><span style=\"font-style: normal  font-weight;\">Leaf-styled waffle pattern lugs on </span><span style=\"font-style: normal  font-weight;\">the forefoot and heel for ample grip, stability and comfort<br /> </span><span style=\"font-style: normal  font-weight;\">Slipper care <br /> </span><span style=\"font-style: normal  font-weight;\">Rub surface with clean, damp cloth to remove dust <br /> Do not machine wash<br /> </span></p>"},"style_note":{"descriptorType":"style_note","value":"<p>NA</p>"}},"styleOptions":[{"id":763707,"name":"Size","value":"C6/7","unifiedSize":"EURO23-24","unifiedSizeValue":"23-24","unifiedSizeScale":"EURO Size","allSize":"UK5-6,C6/7,EURO23-24","skuId":28969,"skuAvailabilityDetailMap":{"1":{"sellerid":48,"storeid":2297,"sellername":"FashionTech","availableCount":0,"availableInWarehouses":"","leadTime":0,"supplyType":"ON_HAND","sellableInventoryCount":0}},"warehouseIdToItemCountMap":{},"inventoryCount":0,"available":false,"active":true},{"id":96490,"name":"Size","value":"C10/11","unifiedSize":"EURO27-28","unifiedSizeValue":"27-28","unifiedSizeScale":"EURO Size","allSize":"UK9-10,C10/11,EURO27-28","skuId":28971,"skuAvailabilityDetailMap":{"1":{"sellerid":48,"storeid":2297,"sellername":"FashionTech","availableCount":0,"availableInWarehouses":"","leadTime":0,"supplyType":"ON_HAND","sellableInventoryCount":0}},"warehouseIdToItemCountMap":{},"inventoryCount":0,"available":false,"active":true},{"id":96491,"name":"Size","value":"C12/13","unifiedSize":"EURO29-31","unifiedSizeValue":"29-31","unifiedSizeScale":"EURO Size","allSize":"UK11-12,C12/13,EURO29-31","skuId":28972,"skuAvailabilityDetailMap":{},"warehouseIdToItemCountMap":{},"inventoryCount":0,"available":false,"active":true},{"id":763706,"name":"Size","value":"J1","unifiedSize":"EURO32-33","unifiedSizeValue":"32-33","unifiedSizeScale":"EURO Size","allSize":"UK13,J1,EURO32-33","skuId":28973,"skuAvailability