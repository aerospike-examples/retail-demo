isReturnable":true,"isExchangeable":true,"pickupEnabled":true,"isTryAndBuyEnabled":true,"isMyntsEnabled":true,"serviceabilityDisclaimer":{"title":"","desc":""}},"isEMIEnabled":true,"otherFlags":[{"dataType":"BOOLEAN","name":"isFragile","value":"false"},{"dataType":"BOOLEAN","name":"isTryAndBuyEnabled","value":"true"},{"dataType":"BOOLEAN","name":"isHazmat","value":"false"},{"dataType":"BOOLEAN","name":"isJewellery","value":"false"},{"dataType":"BOOLEAN","name":"isReturnable","value":"true"},{"dataType":"BOOLEAN","name":"isExchangeable","value":"true"},{"dataType":"BOOLEAN","name":"active","value":"true"},{"dataType":"BOOLEAN","name":"socialSharingEnabled","value":"true"},{"dataType":"BOOLEAN","name":"pickupEnabled","value":"true"},{"dataType":"BOOLEAN","name":"isLarge","value":"false"},{"dataType":"BOOLEAN","name":"codEnabled","value":"true"}],"articleDisplayAttr":{"id":94,"core":{"order":"0","display":"2","pdtDetail":"1","pdtDeliveryOptions":"2","pdtSimilar":"2","pdtLike":"2"},"social":{"order":"1","display":"2","userLikes":"1","styleNotes":"2","crossSell":"1"},"explore":{"display":"2","crosslink":"1","similar":"1","order":"2"}},"productDescriptors":{"description":{"descriptorType":"description","value":"<p><p style=\"text-align: justify;\">Nailing the classic loafer design on the head, the Myrics from rockport is a fabulous slip-on leather shoe designed for the smart you. The horse bit vamp embellishment, stand-up seam and man-made sole with a stack heel - all add to its style element. But the cherry on the icing is the Kinetic Air Circulator footbed with perforations to offer the ultimate in breathability and cushioned comfort.<br /><br /><strong>Upper</strong></p>\n<br/><ul>\n<br/><li>Full grain leather upper with a horse bit vamp embellishment on the tongue for style, comfort and breathability</li>\n<br/><li>Loafer design for easy wearing and removal of the foot</li>\n<br/><li>Stitched seams for abrasion resistance</li>\n<br/><li>Wraparound overlay with an additional overlay at the back foot for absolute support and durability</li>\n<br/></ul>\n<br/><p style=\"text-align: justify;\"><strong>Midsole<br /></strong></p>\n<br/><ul>\n<br/><li>EVA midsole for durability and to conform to the shape of the foot</li>\n<br/><li>Foam footbed with Kinetic Air Circulator technology in the heel and perforations in the forefoot for durable cushioning and dry comfort for all-day use</li>\n<br/><li>Removable footbed to accommodate an orthotic</li>\n<br/></ul>\n<br/><p style=\"text-align: justify;\"><strong>Outsole<br /></strong></p>\n<br/><ul>\n<br/><li>Man-made sole made of PU for lightweight durability</li>\n<br/><li>Textured forefoot and front heel, with horizontally grooved back heel for ample grip and traction on various surfaces</li>\n<br/><li>Brand badge on midfoot for style</li>\n<br/><li>Raised heel with a curved midfoot for ergonomic comfort</li>\n<br/></ul>\n<br/><p style=\"text-align: justify;\"><strong>shoe care<br /></strong>Wipe surface with a dry, clean cloth to remove dust<br />Use a branded leather conditioner and leather brush to restore shine<br />Do not store in direct sunlight</p>"}},"styleOptions":[{"id":27702,"name":"Size","value":"US8","unifiedSize":"UK7","unifiedSizeValue":"7","unifiedSizeScale":"UK Size","allSize":"UK7,US8,EURO40.5","skuId":32942,"skuAvailabilityDetailMap":{},"warehouseIdToItemCountMap":{},"inventoryCount":0,"available":false,"active":true},{"id":27703,"name":"Size","value":"US9","unifiedSize":"UK8","unifiedSizeValue":"8","unifiedSizeScale":"UK Size","allSize":"UK8,US9,EURO42","skuId":32943,"skuAvailabilityDetailMap":{},"warehouseIdToItemCountMap":{},"inventoryCount":0,"available":false,"active":true},{"id":27704,"name":"Size","value":"US10","unifiedSize":"UK9","unifiedSizeValue":"9","unifiedSizeScale":"UK Size","allSize":"UK9,US10,EURO43","skuId":32944,"skuAvailabilityDetailMap":{},"warehouseIdToItemCountMap":{},"inventoryCount":0,"available":false,"active":true},{"id":27705,"name":"Size","valu