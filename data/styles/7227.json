ingEnabled":true,"isReturnable":true,"isExchangeable":true,"pickupEnabled":true,"isTryAndBuyEnabled":true,"isMyntsEnabled":true,"serviceabilityDisclaimer":{"title":"","desc":""}},"isEMIEnabled":true,"otherFlags":[{"dataType":"BOOLEAN","name":"isFragile","value":"false"},{"dataType":"BOOLEAN","name":"isTryAndBuyEnabled","value":"true"},{"dataType":"BOOLEAN","name":"isHazmat","value":"false"},{"dataType":"BOOLEAN","name":"isJewellery","value":"false"},{"dataType":"BOOLEAN","name":"isReturnable","value":"true"},{"dataType":"BOOLEAN","name":"isExchangeable","value":"true"},{"dataType":"BOOLEAN","name":"active","value":"true"},{"dataType":"BOOLEAN","name":"socialSharingEnabled","value":"true"},{"dataType":"BOOLEAN","name":"pickupEnabled","value":"true"},{"dataType":"BOOLEAN","name":"isLarge","value":"false"},{"dataType":"BOOLEAN","name":"codEnabled","value":"true"}],"articleDisplayAttr":{"id":94,"core":{"order":"0","display":"2","pdtDetail":"1","pdtDeliveryOptions":"2","pdtSimilar":"2","pdtLike":"2"},"social":{"order":"1","display":"2","userLikes":"1","styleNotes":"2","crossSell":"1"},"explore":{"display":"2","crosslink":"1","similar":"1","order":"2"}},"productDescriptors":{"description":{"descriptorType":"description","value":"<p><p style=\"text-align: justify;\">Talk about walking comfort in dress shoes, and there's no better brand that can offer it than rockport. If you want to experience this, then slip your feet into the Wanigan oxfords. With a smooth leather upper and a heavily padded insole with adiPRENE technology, the Wanigan is a perfect footwear for those long hours at work.<br /><br /><strong>Upper</strong></p>\n<br/><ul>\n<br/><li>Full grain leather upper with a smooth finish and curvy stitching for ample breathability to offer fresh, dry comfort</li>\n<br/><li>Leather wrap around overlay for enhanced shoe life</li>\n<br/><li>Long tongue with a padded collar for snug comfort</li>\n<br/><li>Lace-up system for snug, customised Fit</li>\n<br/><li>Nubuck leather lining for a dry, sweat free wear</li>\n<br/></ul>\n<br/><p style=\"text-align: justify;\"><strong>Midsole</strong></p>\n<br/><ul>\n<br/><li>EVA midsole for durability and to conform to the shape of the foot</li>\n<br/><li>adiPRENE deep cushioning insole that offers a light, responsive ride and for maximum shock absorption offering a resilient ride</li>\n<br/><li>Removable footbed to accommodate an orthotic</li>\n<br/></ul>\n<br/><p style=\"text-align: justify;\"><strong>Outsole</strong></p>\n<br/><ul>\n<br/><li>Durable PU outsole with an impact guidance line from the midfoot to the forefoot for added protection from impact and easy flexibility</li>\n<br/><li>Strategically grooved forefoot and back heel in a diamond shape for absolute grip on various surfaces</li>\n<br/><li>Textured fore heel for enhanced grip and traction</li>\n<br/><li>Curved and textured medial midfoot for inner arch support</li>\n<br/><li>Flex grooves in the forefoot for easy movement</li>\n<br/><li>Raised heel for ergonomic comfort and style</li>\n<br/></ul>\n<br/><p style=\"text-align: justify;\"><strong>Shoe care</strong><br />Wipe surface with a dry, clean cloth to remove dust<br />Use a branded leather conditioner and leather brush to restore shine<br />Do not store in direct sunlight</p>"}},"styleOptions":[{"id":27689,"name":"Size","value":"US8","unifiedSize":"UK7","unifiedSizeValue":"7","unifiedSizeScale":"UK Size","allSize":"UK7,US8,EURO40.5","skuId":33138,"skuAvailabilityDetailMap":{},"warehouseIdToItemCountMap":{},"inventoryCount":0,"available":false,"active":true},{"id":27690,"name":"Size","value":"US9","unifiedSize":"UK8","unifiedSizeValue":"8","unifiedSizeScale":"UK Size","allSize":"UK8,US9,EURO42","skuId":33139,"skuAvailabilityDetailMap":{},"warehouseIdToItemCountMap":{},"inventoryCount":0,"available":false,"active":true},{"id":27691,"name":"Size","value":"US10","unifiedSize":"UK9","unifiedSizeValue":"9","unifiedSizeScale":"UK Size","allSize":"UK9,US10,EURO43","skuId":33140,"skuAv