haringEnabled":true,"isReturnable":true,"isExchangeable":true,"pickupEnabled":true,"isTryAndBuyEnabled":true,"isMyntsEnabled":true,"serviceabilityDisclaimer":{"title":"","desc":""}},"isEMIEnabled":true,"otherFlags":[{"dataType":"BOOLEAN","name":"isFragile","value":"false"},{"dataType":"BOOLEAN","name":"isTryAndBuyEnabled","value":"true"},{"dataType":"BOOLEAN","name":"isHazmat","value":"false"},{"dataType":"BOOLEAN","name":"isJewellery","value":"false"},{"dataType":"BOOLEAN","name":"isReturnable","value":"true"},{"dataType":"BOOLEAN","name":"isExchangeable","value":"true"},{"dataType":"BOOLEAN","name":"active","value":"true"},{"dataType":"BOOLEAN","name":"socialSharingEnabled","value":"true"},{"dataType":"BOOLEAN","name":"pickupEnabled","value":"true"},{"dataType":"BOOLEAN","name":"isLarge","value":"false"},{"dataType":"BOOLEAN","name":"codEnabled","value":"true"}],"articleDisplayAttr":{"id":94,"core":{"order":"0","display":"2","pdtDetail":"1","pdtDeliveryOptions":"2","pdtSimilar":"2","pdtLike":"2"},"social":{"order":"1","display":"2","userLikes":"1","styleNotes":"2","crossSell":"1"},"explore":{"display":"2","crosslink":"1","similar":"1","order":"2"}},"productDescriptors":{"description":{"descriptorType":"description","value":"<p><p style=\"text-align: justify;\">Talk about walking comfort in dress shoes, and there's no better brand that can offer it than rockport. To experience the delightful feeling of walking on air all day long, slip your feet into the Mepozo oxfords from rockport's Signature Series. Engineered with Dynamic Suspension technology, it is the perfect pair of formal shoes for those long days at work. A great addition to any sophisticated man's wardrobe, it can add tons of character to your outfit in an instant.<br /><br /><strong>Upper</strong></p>\n<br/><ul>\n<br/><li>Full-grain leather upper for added durability and style</li>\n<br/><li>Perforations for better ventilation and style</li>\n<br/><li>Fabric lining for better breathability</li>\n<br/><li>Plain toe design for a neater, crisper look and extra durability</li>\n<br/><li>Lace-up system with synthetic laces for a snug, customized Fit</li>\n<br/><li>Lightly padded tongue to protect from excessive rubbing and prevent blisters</li>\n<br/><li>Leather wrap-around overlay along the heel and midfoot for better support</li>\n<br/></ul>\n<br/><p style=\"text-align: justify;\"><strong>Midsole<br /></strong></p>\n<br/><ul>\n<br/><li>EVA midsole with shock absorbers for lightweight comfort</li>\n<br/><li>Dynamic Suspension technology that creates a thin air cushion under the foot, moving according to the pressure of your foot from heel to toe, ensuring maximum comfort even with the slightest movement </li>\n<br/></ul>\n<br/><p style=\"text-align: justify;\"><strong>Outsole<br /></strong></p>\n<br/><ul>\n<br/><li>PU outsole for durability and shock absorption to help reduce foot fatigue</li>\n<br/><li>Stacked heel for a better grip</li>\n<br/><li>Textured pattern on the heel, midfoot and forefoot for better grip</li>\n<br/><li>Flex grooves on the forefoot and heel for ease of movement and improving grip on various surfaces</li>\n<br/></ul>\n<br/><p style=\"text-align: justify;\"><strong>shoe care<br /></strong>Wipe surface with a dry, clean cloth to remove dust<br />Use a branded leather conditioner and leather brush to restore shine<br />Do not store in direct sunlight</p>"}},"styleOptions":[{"id":27792,"name":"Size","value":"US8","unifiedSize":"UK7","unifiedSizeValue":"7","unifiedSizeScale":"UK Size","allSize":"UK7,US8,EURO40.5","skuId":32914,"skuAvailabilityDetailMap":{},"warehouseIdToItemCountMap":{},"inventoryCount":0,"available":false,"active":true},{"id":27793,"name":"Size","value":"US9","unifiedSize":"UK8","unifiedSizeValue":"8","unifiedSizeScale":"UK Size","allSize":"UK8,US9,EURO42","skuId":32915,"skuAvailabilityDetailMap":{},"warehouseIdToItemCountMap":{},"inventoryCount":0,"available":false,"active":true},{"id":27794,"name":"Size","value":"US10","unifiedS