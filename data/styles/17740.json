ex grooves built across the waffle patterned outsole to enhance flexibility <br /> <br /> </span><span style=\"font-style: normal  font-weight;\">Wash care</span><span style=\"font-style: normal  font-weight;\"> <br /> Wipe surface with soft, clean, damp cloth to remove dust <br /> Do not store in direct sunlight</span></p>"}},"styleOptions":[{"id":80245,"name":"Size","value":"UK7","unifiedSize":"UK7","unifiedSizeValue":"7","unifiedSizeScale":"UK Size","allSize":"UK7,US8,EURO40.5","skuId":93317,"skuAvailabilityDetailMap":{},"warehouseIdToItemCountMap":{},"inventoryCount":0,"available":false,"active":true},{"id":80244,"name":"Size","value":"UK8","unifiedSize":"UK8","unifiedSizeValue":"8","unifiedSizeScale":"UK Size","allSize":"UK8,US9,EURO42","skuId":93318,"skuAvailabilityDetailMap":{},"warehouseIdToItemCountMap":{},"inventoryCount":0,"available":false,"active":true},{"id":80243,"name":"Size","value":"UK9","unifiedSize":"UK9","unifiedSizeValue":"9","unifiedSizeScale":"UK Size","allSize":"UK9,US10,EURO43","skuId":93319,"skuAvailabilityDetailMap":{},"warehouseIdToItemCountMap":{},"inventoryCount":0,"available":false,"active":true},{"id":80166,"name":"Size","value":"UK10","unifiedSize":"UK10","unifiedSizeValue":"10","unifiedSizeScale":"UK Size","allSize":"UK10,US11,EURO44.5","skuId":93320,"skuAvailabilityDetailMap":{},"warehouseIdToItemCountMap":{},"inventoryCount":0,"available":false,"active":true}]}}