rnable":true,"isExchangeable":true,"pickupEnabled":true,"isTryAndBuyEnabled":true,"isMyntsEnabled":true,"serviceabilityDisclaimer":{"title":"","desc":""}},"isEMIEnabled":true,"otherFlags":[{"dataType":"BOOLEAN","name":"isFragile","value":"false"},{"dataType":"BOOLEAN","name":"isTryAndBuyEnabled","value":"true"},{"dataType":"BOOLEAN","name":"isHazmat","value":"false"},{"dataType":"BOOLEAN","name":"isJewellery","value":"false"},{"dataType":"BOOLEAN","name":"isReturnable","value":"true"},{"dataType":"BOOLEAN","name":"isExchangeable","value":"true"},{"dataType":"BOOLEAN","name":"active","value":"true"},{"dataType":"BOOLEAN","name":"socialSharingEnabled","value":"true"},{"dataType":"BOOLEAN","name":"pickupEnabled","value":"true"},{"dataType":"BOOLEAN","name":"isLarge","value":"false"},{"dataType":"BOOLEAN","name":"codEnabled","value":"true"}],"articleDisplayAttr":{"id":94,"core":{"order":"0","display":"2","pdtDetail":"1","pdtDeliveryOptions":"2","pdtSimilar":"2","pdtLike":"2"},"social":{"order":"1","display":"2","userLikes":"1","styleNotes":"2","crossSell":"1"},"explore":{"display":"2","crosslink":"1","similar":"1","order":"2"}},"productDescriptors":{"description":{"descriptorType":"description","value":"<p><p style=\"text-align: justify;\">rockport Alfrew is the first-ever classic dress shoe engineered with an athletic sole. Made for professional men who love their formal wear and want to look sharp and crisp, these shoes are lighter than traditional <a href='/running-shoes?src=desc' class='seolink'>running shoes</a>, but come with great shock absorbers allowing you to run from one meeting to another without any worry of stress, slips and sprains. Replete with a smooth, flawless upper and a plain toe silhouette, it's a great addition to any sophisticated man's wardrobe.<br /><strong><br />Upper</strong></p>\n<br/><ul>\n<br/><li>Full-grain leather upper for added durability and style</li>\n<br/><li>Nappa leather lining for moisture absorption and abrasion resistance</li>\n<br/><li>Plain toe design for a neater, crisper look and extra durability</li>\n<br/><li>Strobel construction for added stability and forefoot flexibility</li>\n<br/><li>Lace-up system with synthetic laces for a snug, customized Fit</li>\n<br/><li>Mesh inner for ample breathability</li>\n<br/><li>Lightly padded tongue with perforations for better ventilation</li>\n<br/><li>Leather wraparound overlay along the heel and midfoot for better support</li>\n<br/><li>Brand badge on lateral ankle</li>\n<br/></ul>\n<br/><p style=\"text-align: justify;\"><strong>Midsole</strong></p>\n<br/><ul>\n<br/><li>EVA midsole with shock absorbers for lightweight comfort</li>\n<br/><li>Moisture-absorbent and odour-resistant ortholite footbed for all-day comfort</li>\n<br/><li>Removable footbed to accomodate an orthotic</li>\n<br/></ul>\n<br/><p style=\"text-align: justify;\"><strong>Outsole</strong></p>\n<br/><ul>\n<br/><li>Lightweight, slip-resistant rubber outsole delivers shock absorption, reducing foot fatigue</li>\n<br/><li>Horizontal grooves through out for better grip</li>\n<br/><li>Rectangular grooves on the lateral heel, midfoot and medial forefoot for improved grip and traction on various surfaces</li>\n<br/><li>Horse shoe pattern with scooped heel for enhanced cushioing of the heel</li>\n<br/><li>Flex grooves in the forefoot for easy movement </li>\n<br/><li>Raised heel for a stylish look and ergonomic comfort</li>\n<br/></ul>\n<br/><p style=\"text-align: justify;\"><strong>Shoe care</strong><br />Wipe surface with a dry, clean cloth to remove dust<br />Use a branded leather conditioner and leather brush to restore shine<br />Do not store in direct sunlight</p>"}},"styleOptions":[{"id":27812,"name":"Size","value":"US8","unifiedSize":"UK7","unifiedSizeValue":"7","unifiedSizeScale":"UK Size","allSize":"UK7,US8,EURO40.5","skuId":33089,"skuAvailabilityDetailMap":{},"warehouseIdToItemCountMap":{},"inventoryCount":0,"available":false,"active":true},{"id":27813,"name":"Size","value"