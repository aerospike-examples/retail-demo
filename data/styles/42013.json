sReturnable":true,"isExchangeable":true,"pickupEnabled":true,"isTryAndBuyEnabled":true},"subCategory":{"id":33,"typeName":"Shoes","active":true,"socialSharingEnabled":true,"isReturnable":true,"isExchangeable":true,"pickupEnabled":true,"isTryAndBuyEnabled":true},"articleType":{"id":95,"typeName":"Sports Shoes","active":true,"socialSharingEnabled":true,"isReturnable":true,"isExchangeable":true,"pickupEnabled":true,"isTryAndBuyEnabled":true,"isMyntsEnabled":true,"serviceabilityDisclaimer":{"title":"","desc":""}},"isEMIEnabled":true,"otherFlags":[{"dataType":"BOOLEAN","name":"isFragile","value":"false"},{"dataType":"BOOLEAN","name":"isTryAndBuyEnabled","value":"true"},{"dataType":"BOOLEAN","name":"isHazmat","value":"false"},{"dataType":"BOOLEAN","name":"isJewellery","value":"false"},{"dataType":"BOOLEAN","name":"isReturnable","value":"true"},{"dataType":"BOOLEAN","name":"isExchangeable","value":"true"},{"dataType":"BOOLEAN","name":"active","value":"true"},{"dataType":"BOOLEAN","name":"socialSharingEnabled","value":"true"},{"dataType":"BOOLEAN","name":"pickupEnabled","value":"true"},{"dataType":"BOOLEAN","name":"isLarge","value":"false"},{"dataType":"BOOLEAN","name":"codEnabled","value":"true"}],"articleDisplayAttr":{"id":95,"core":{"order":"0","display":"2","pdtDetail":"2","pdtDeliveryOptions":"2","pdtSimilar":"2","pdtLike":"2"},"social":{"order":"1","display":"2","userLikes":"1","styleNotes":"2","crossSell":"1"},"explore":{"display":"2","crosslink":"1","similar":"1","order":"2"}},"productDescriptors":{"description":{"descriptorType":"description","value":"<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\"><strong>Style Note</strong></span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Sleek and yet aggressively designed, the nike Ballista II shoe is replete with comfort and style. Ergonomically designed and well cushioned, it's a great shoe for runs and casual occasions.</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\"><br /></span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\"><strong>Product Details</strong></span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Meshed upper for ample breathability</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Perforated synthetic overlays for structure, midfoot support and ventilation</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Asymmetrical lacing system for snug Fit and protection from chaffing</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Twill lace running through mid and back foot overlays for enhanced durability</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Synthetic laces for durability with a plush padded tongue for enhanced comfort</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Cushioned sockliner for in-step comfort and snug fit</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Reinforced overlays on heel cup and above toe guard for enhanced durability and protection</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Cushioned footbed for comfort and impact protection</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">EVA midsole for durability and enhanced shock absorption and a padded back sole to lock the foot in place</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">High quality rubber outsole for enhanced durability</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Forefoot and backfoot outsole with rectangular lugs and grooves for maximum traction and grip</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Impact guidance system along the length of the foot for ample impact protection, energy return and propulsion</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Deep flex groove on mid forefoot for flexible movement and an arched midfoot to support the inner arch</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Forefoot outsole reinforced onto the upper as a toe box for enhanced protection to the forefoot<br />Warranty for manufacturing defects: 6 months (not valid on products with more than 20% discount)&nbsp; </span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\"><br /></span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\"><strong>Material and Care</strong></span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Synthetic</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Do not machine wash</span></span></div>\n<div><span style=\"font-family: tahoma, arial, helvetica, sans-serif;\"><span style=\"font-size: 12px;\">Remove d